! -*- coding: utf-8 -*-
! University of Zurich - Institute of Computational Linguistics
! Morphology analysis for Romansh: Interpunctation
! Author: Reto Baumgartner
! Date: June 2013
! Last change: July 2015


Multichar_Symbols +Sent +CM +Punct +End +Beg +Symbol

LEXICON Root
%.	SENT;
%,	COMMA;
%;	SENT;
%:	SENT;
%?	SENT;
%!	SENT;
%-	PUNCT;
%–	PUNCT;
%/	PUNCT;
%(	PUNCTBEG;
%)	PUNCTEND;
%[	PUNCTBEG;
%]	PUNCTEND;
%{	PUNCTBEG;
%}	PUNCTEND;
%*	PUNCT;
%~	PUNCT;
%_	PUNCT;
%«	PUNCTBEG;
%»	PUNCTEND;
%‹	PUNCTBEG;
%›	PUNCTEND;
%„	PUNCTBEG;
%“	PUNCTEND;
%“	PUNCTBEG;
%”	PUNCTEND;
%‚	PUNCTBEG;
%‘	PUNCTEND;
%‘	PUNCTBEG;
%’	PUNCTEND;
%%	PUNCT;
%…  PUNCT;
%"	PUNCT;
%=	SYMBOL;

LEXICON SENT
+Sent:0	#;

LEXICON COMMA
+CM:0	#;

LEXICON PUNCT
+Punct:0	#;

LEXICON PUNCTBEG
+Punct+Beg:0	#;

LEXICON PUNCTEND
+Punct+End:0	#;

LEXICON SYMBOL
+Symbol:0	#;
